name: "ResNet-18"
layer {
   name: "data"
   type: "Input"
   top: "data"
   input_param {
     shape:{
     dim:1
     dim:3
     dim:224
     dim:224
    }
   }
 }

layer {
    bottom: "data"
    top: "conv1"
    name: "conv1"
    type: "Convolution"
    convolution_param {
        num_output: 64
        kernel_size: 7
        pad: 3
        stride: 2
        weight_filler {
            type: "msra"
        }
        bias_term: false

    }
}

layer {
    bottom: "conv1"
