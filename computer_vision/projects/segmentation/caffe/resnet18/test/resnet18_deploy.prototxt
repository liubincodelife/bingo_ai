        lr_mult: 1.0
        decay_mult: 1
    }
}

###--------concat---------
layer {
  bottom: "res4b"
  top: "deconv_res4b"
  name: "deconv_res4b"
  type: "Convolution"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 64
    pad: 1
    stride: 1
    kernel_size: 3
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
